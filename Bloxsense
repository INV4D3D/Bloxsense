		Config["Other border"].Visible = true

		Aim.Icon.IMG.ImageColor3 = Color3.fromRGB(143, 143, 143) 
		AA.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Legit.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Visuals.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Settings.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Skins.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Config.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)

		AimToggled = false
		AAToggled = false
		LegitToggled = false
		VisualToggled = false
		SettingsToggled = false
		SkinsToggled = false
		UserToggled = true
		ConfigToggled = false
		show(User)		
	end
	
	if Config.Icon.IMG.ImageColor3 == Color3.fromRGB(255, 255, 255) and ConfigToggled == false then
		Aim.Main.Visible = false
		AA.Main.Visible = false
		Legit.Main.Visible = false
		Visuals.Main.Visible = false
		Settings.Main.Visible = false
		Skins.Main.Visible = false
		User.Main.Visible = false
		Config.Main.Visible = true
		
		Aim.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		AA.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Legit.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Visuals.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Settings.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Skins.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		User.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Config.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		
		Aim["Side Border"].Visible = true
		AA["Side Border"].Visible = true
		Legit["Side Border"].Visible = true
		Visuals["Side Border"].Visible = true
		Settings["Side Border"].Visible = true
		Skins["Side Border"].Visible = true
		User["Side Border"].Visible = true
		Config["Side Border"].Visible = false

		AA["Other border"].Visible = false
		Legit["Other border"].Visible = false
		Visuals["Other border"].Visible = false
		Settings["Other border"].Visible = false
		Skins["Other border"].Visible = false
		User["Other border"].Visible = false
		Config["Other border"].Visible = true

		Aim.Icon.IMG.ImageColor3 = Color3.fromRGB(143, 143, 143) 
		AA.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Legit.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Visuals.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Settings.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		Skins.Icon.IMG.ImageColor3  = Color3.fromRGB(143, 143, 143)
		User.Icon.IMG.ImageColor3 = Color3.fromRGB(143, 143, 143)

		AimToggled = false
		AAToggled = false
		LegitToggled = false
		VisualToggled = false
		SettingsToggled = false
		SkinsToggled = false
		UserToggled = false
		ConfigToggled = true
		show(Config)
	end
	wait(1/1000)
end
end))
