				script.Parent.Parent.Parent.Parent.OtherTab.Pic2.Image = "https://www.roblox.com/bust-thumbnail/image?userId=" ..Player.UserId.."&width=420&height=420&format=png"

			end

		end
		button.Activated:Connect(onButtonActivated)
		--	end
	end

end)

