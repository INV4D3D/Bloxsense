LocalScript816.Name = "Fakelag"
LocalScript816.Parent = ImageButton814
table.insert(cors,sandbox(LocalScript816,function()
local button = script.Parent
local toggled = false
local choking = false
local player = game.Players.LocalPlayer

function choketick()
	game:GetService("NetworkClient"):SetOutgoingKBPSLimit(1)
	script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.TextColor3 = Color3.fromRGB(165, 255, 92)
	choking = true	
end

function stopchoke()
	game:GetService("NetworkClient"):SetOutgoingKBPSLimit(9e9)
	script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.TextColor3 = Color3.fromRGB(255, 63, 25)
	choking = false
end

spawn(function()
	while wait(1/16) do
		if toggled == true then
			if script.Parent.Parent.Amount.Selection.Text == "Fluctuate" then
				choketick()
				wait(math.random(((script.Parent.Parent.LIM.Percent.Value/7.14285714)/20), math.clamp((script.Parent.Parent.VAR.Percent.Value)/100)))
				stopchoke()
				wait(math.random(0,math.clamp((script.Parent.Parent.VAR.Percent.Value), 1, 100))/100)
			else
				choketick()
				wait(((script.Parent.Parent.LIM.Percent.Value/7.14285714)/20))
				stopchoke()
				wait(2/16)
			end
		end
	end	
end)

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		stopchoke()
	end
end
button.Activated:Connect(onButtonActivated)
end))
