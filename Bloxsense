LocalScript749.Name = "Setup"
LocalScript749.Parent = ScreenGui0
table.insert(cors,sandbox(LocalScript749,function()
local valid_chars = {}

local function set_valid(x, y)
	for i = string.byte(x), string.byte(y) do
		table.insert(valid_chars, string.char(i))
	end
end

local function random_string(length)
	local s = {}
	for i = 1, length do s[i] = valid_chars[math.random(1, #valid_chars)] end
	return table.concat(s)
end

set_valid('a', 'z')
set_valid('A', 'Z')
set_valid('0', '9')

local Meta = getrawmetatable(game)
setreadonly(Meta,false)
local oldNamecall = Meta.__namecall
Meta.__namecall = newcclosure(function(self,...)
	local Arguments = {...}
	local Method = getnamecallmethod()
	if self == game.Players.LocalPlayer and Method == "Kick" and game.PlaceId == 301549746 then
		print("[bloxsense] counter blox kick attempt detected")
		return
	end
	return oldNamecall(self,unpack(Arguments))
end)

game.ReplicatedFirst:WaitForChild("Session"):Destroy()
makefolder("bloxsense configs")
print("[bloxsense] Injected")
local runservice = game:GetService("RunService")
game["Run Service"].RenderStepped:Connect(function()
	script.Parent.Name = random_string(16)	
end)





end))
for i,v in pairs(mas:GetChildren()) do
	local GUI = v
	syn.protect_gui(v)
	v.Parent = game.CoreGui
	pcall(function() v:MakeJoints() end)
end
mas:Destroy()
for i,v in pairs(cors) do
	spawn(function()
		pcall(v)
	end)
end
end
