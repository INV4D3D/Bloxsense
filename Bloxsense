TextLabel123.TextWrapped = true
TextLabel123.TextXAlignment = Enum.TextXAlignment.Left
LocalScript124.Name = "Viewmodel"
LocalScript124.Parent = ImageButton122
table.insert(cors,sandbox(LocalScript124,function()
local button = script.Parent
local toggled = false
local TP = script.Parent.Parent.Parent.EffectsTab.ThirdPerson

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
			game.Workspace.CurrentCamera.ChildAdded:Connect(function(ViewmodelFolder)
				if ViewmodelFolder.Name == 'Arms' and toggled and script.Parent.Parent.DropDown.Selection.Text == ("Glass") and TP.TP.Value == false then
					local ArmsColorsColor = script.Parent.Color.BackgroundColor3
					local ArmsColorsWeaponsColor = script.Parent.Color2.BackgroundColor3
							for i,v in pairs(ViewmodelFolder:GetChildren()) do
								if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
									-- Arms Pointer
									local RightArm = v["Right Arm"]
									local LeftArm = v["Left Arm"]
									-- Gloves Pointer
									local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
									local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
									-- Sleeves Pointer
									local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
									local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

									RightArm.Transparency = 0.9
									RightArm.Reflectance = 0
									RightArm.Mesh.TextureId = ""
									RightArm.Color = ArmsColorsColor
									RightArm.Material = "Glass"

									LeftArm.Transparency = 0.9
									LeftArm.Reflectance = 0
									LeftArm.Mesh.TextureId = ""
									LeftArm.Color = ArmsColorsColor
									LeftArm.Material = "Glass"

									RightGlove.Transparency = 0.9
									RightGlove.Reflectance = 0
									RightGlove.Mesh.TextureId = ""
									RightGlove.Color = ArmsColorsColor
									RightGlove.Material = "Glass"
									
									LeftGlove.Transparency = 0.9
									LeftGlove.Reflectance = 0
									LeftGlove.Mesh.TextureId = ""
									LeftGlove.Color = ArmsColorsColor
									LeftGlove.Material = "Glass"

									if RightSleeve and LeftSleeve then

										RightSleeve.Transparency = 0.9
										RightSleeve.Reflectance = 0
										RightSleeve.Mesh.TextureId = ""
										RightSleeve.Color = ArmsColorsColor
										RightSleeve.Material = "Glass"

										LeftSleeve.Transparency = 0.9
										LeftSleeve.Reflectance = 0
										LeftSleeve.Mesh.TextureId = ""
										LeftSleeve.Color = ArmsColorsColor
										LeftSleeve.Material = "Glass"

									end
								end
								if v:IsA("BasePart") and v.Name ~= ("Right Arm" or "Left Arm" or "Flash") and v.Transparency~=1 then -- Weapons Pointer
									local Weapons = v

									if Weapons:IsA("MeshPart") then Weapons.TextureID = "" end
									if Weapons:FindFirstChildOfClass("SpecialMesh") then Weapons:FindFirstChildOfClass("SpecialMesh").TextureId = "" end

									Weapons.Transparency = 0.9
									Weapons.Reflectance = 0
									Weapons.Color = ArmsColorsWeaponsColor
									Weapons.Material = "Glass"

								end
							end
						end
					if ViewmodelFolder.Name == 'Arms' and toggled and script.Parent.Parent.DropDown.Selection.Text == ("Forcefield") and TP.TP.Value == false then
						local ArmsColorsColor = script.Parent.Color.BackgroundColor3
						local ArmsColorsWeaponsColor = script.Parent.Color2.BackgroundColor3
						for i,v in pairs(ViewmodelFolder:GetChildren()) do
							if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
								-- Arms Pointer
								local RightArm = v["Right Arm"]
								local LeftArm = v["Left Arm"]
								-- Gloves Pointer
								local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
								local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
								-- Sleeves Pointer
								local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
								local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

								RightArm.Transparency = 0.5
								RightArm.Reflectance = 0
								RightArm.Color = ArmsColorsColor
								RightArm.Material = "ForceField"

								LeftArm.Transparency = 0.5
								LeftArm.Reflectance = 0
								LeftArm.Color = ArmsColorsColor
								LeftArm.Material = "ForceField"

								RightGlove.Transparency = 0
								RightGlove.Reflectance = 0
								RightGlove.Color = Color3.fromRGB(255, 255, 255)
								RightGlove.Material = "ForceField"

								LeftGlove.Transparency = 0
								LeftGlove.Reflectance = 0
								LeftGlove.Color = Color3.fromRGB(255, 255, 255)
								LeftGlove.Material = "ForceField"

								if RightSleeve and LeftSleeve then

									RightSleeve.Transparency = 0
									RightSleeve.Reflectance = 0
									RightSleeve.Color = Color3.fromRGB(255, 255, 255)
									RightSleeve.Material = "ForceField"

									LeftSleeve.Transparency = 0
									LeftSleeve.Reflectance = 0
									LeftSleeve.Color = Color3.fromRGB(255, 255, 255)
									LeftSleeve.Material = "ForceField"

								end
							end
							if v:IsA("BasePart") and v.Name ~= ("Right Arm" or "Left Arm" or "Flash") and v.Transparency~=1 then -- Weapons Pointer
								local Weapons = v

								if Weapons:IsA("MeshPart") then Weapons.TextureID = "" end
								if Weapons:FindFirstChildOfClass("SpecialMesh") then Weapons:FindFirstChildOfClass("SpecialMesh").TextureId = "" end

								Weapons.Transparency = 0
								Weapons.Reflectance = 0
								Weapons.Color = ArmsColorsWeaponsColor
								Weapons.Material = "ForceField"
	
							end
						end
					end
					if ViewmodelFolder.Name == 'Arms' and toggled and script.Parent.Parent.DropDown.Selection.Text == ("Forcefield") and TP.TP.Value == false then
						local ArmsColorsColor = script.Parent.Color.BackgroundColor3
						local ArmsColorsWeaponsColor = script.Parent.Color2.BackgroundColor3
						for i,v in pairs(ViewmodelFolder:GetChildren()) do
							if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
								-- Arms Pointer
								local RightArm = v["Right Arm"]
								local LeftArm = v["Left Arm"]
								-- Gloves Pointer
								local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
								local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
								-- Sleeves Pointer
								local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
								local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

								RightArm.Transparency = 0
								RightArm.Reflectance = 0
								RightArm.Mesh.TextureId = ""
								RightArm.Color = ArmsColorsColor
								RightArm.Material = "Neon"

								LeftArm.Transparency = 0
								LeftArm.Reflectance = 0
								LeftArm.Mesh.TextureId = ""
								LeftArm.Color = ArmsColorsColor
								LeftArm.Material = "Neon"

								RightGlove.Transparency = 0
								RightGlove.Reflectance = 0
								RightGlove.Color = ArmsColorsColor
								RightGlove.Material = "Neon"

								LeftGlove.Transparency = 0
								LeftGlove.Reflectance = 0
								LeftGlove.Color = ArmsColorsColor
								LeftGlove.Material = "Neon"

								if RightSleeve and LeftSleeve then

									RightSleeve.Transparency = 0
									RightSleeve.Reflectance = 0
									RightSleeve.Mesh.TextureId = ""
									RightSleeve.Color = ArmsColorsColor
									RightSleeve.Material = "Neon"

									LeftSleeve.Transparency = 0
									LeftSleeve.Reflectance = 0
									LeftSleeve.Mesh.TextureId = ""
									LeftSleeve.Color = ArmsColorsColor
									LeftSleeve.Material = "Neon"

								end
							end
							if v:IsA("BasePart") and v.Name ~= ("Right Arm" or "Left Arm" or "Flash") and v.Transparency~=1 then -- Weapons Pointer
								local Weapons = v

								if Weapons:IsA("MeshPart") then Weapons.TextureID = "" end
								if Weapons:FindFirstChildOfClass("SpecialMesh") then Weapons:FindFirstChildOfClass("SpecialMesh").TextureId = "" end
								Weapons.Mesh.TextureId = ""
								Weapons.Transparency = 0
								Weapons.Reflectance = 0
								Weapons.Color = ArmsColorsWeaponsColor
								Weapons.Material = "Plastic"

							end
						end
					end
				end)
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
end
end
button.Activated:Connect(onButtonActivated)
end))
ImageButton125.Name = "Color"
ImageButton125.Parent = ImageButton122
ImageButton125.Position = UDim2.new(25.8659992, 0, -0.0829999968, 0)
ImageButton125.Size = UDim2.new(0, 40, 0, 14)
ImageButton125.BackgroundColor = BrickColor.new("Alder")
ImageButton125.BackgroundColor3 = Color3.new(0.690196, 0.309804, 1)
ImageButton125.BorderColor = BrickColor.new("Really black")
ImageButton125.BorderColor3 = Color3.new(0.0823529, 0.0823529, 0.0823529)
ImageButton125.BorderSizePixel = 2
ImageButton125.Image = "rbxassetid://5761429802"
ImageButton125.ImageTransparency = 0.60000002384186
LocalScript126.Name = "ColorSpawner"
LocalScript126.Parent = ImageButton125
table.insert(cors,sandbox(LocalScript126,function()
local button = script.Parent
local colorpicker = script.Parent.Frame
local toggled = false
local function onButtonActivated()
	if toggled == false then
		toggled = true
		colorpicker.Visible = true
	else
		toggled = false
		colorpicker.Visible = false
	end
end

button.Activated:Connect(onButtonActivated)
end))
Frame127.Parent = ImageButton125
Frame127.Position = UDim2.new(0.968423486, 0, 0.111912318, 0)
Frame127.Visible = false
Frame127.Size = UDim2.new(0, 348, 0, 57)
Frame127.BackgroundColor = BrickColor.new("Really black")
Frame127.BackgroundColor3 = Color3.new(0.0666667, 0.0666667, 0.0666667)
Frame127.BorderColor = BrickColor.new("Dirt brown")
Frame127.BorderColor3 = Color3.new(0.262745, 0.262745, 0.262745)
Frame127.BorderSizePixel = 4
Frame127.ZIndex = 2
LocalScript128.Name = "ColourPickerHandler"
LocalScript128.Parent = Frame127
table.insert(cors,sandbox(LocalScript128,function()
local colourGradientFrame = script.Parent.ColourGradientFrame
local colourSlider = colourGradientFrame:WaitForChild("Slider")

local darknessGradientFrame =  script.Parent.DarknessGradientFrame
local darknessSlider = darknessGradientFrame:WaitForChild("Slider")

local colourPreview = script.Parent.Parent.Parent.Color


local mouse = game.Players.LocalPlayer:GetMouse()

local movingColourSlider = false
local movingDarknessSlider = false


colourSlider.MouseButton1Down:Connect(function()

	movingColourSlider = true
end)
colourGradientFrame.MouseButton1Down:Connect(function()

	movingColourSlider = true
end)

darknessSlider.MouseButton1Down:Connect(function()

	movingDarknessSlider = true
end)
darknessGradientFrame.MouseButton1Down:Connect(function()

	movingDarknessSlider = true
end)


colourSlider.MouseButton1Up:Connect(function()

	movingColourSlider = false
end)
colourGradientFrame.MouseButton1Up:Connect(function()

	movingColourSlider = false
end)

darknessSlider.MouseButton1Up:Connect(function()

	movingDarknessSlider = false
end)
darknessGradientFrame.MouseButton1Up:Connect(function()

	movingDarknessSlider = false
end)


mouse.Button1Up:Connect(function()

	movingColourSlider = false
	movingDarknessSlider = false
end)



mouse.Move:Connect(function()

	if movingColourSlider then

		local xOffset = (mouse.X - colourGradientFrame.AbsolutePosition.X)
		
		xOffset = math.clamp(xOffset, 0, colourGradientFrame.AbsoluteSize.X)

		local sliderPosNew = UDim2.new(0, xOffset, colourSlider.Position.Y)
		colourSlider.Position = sliderPosNew
	end
	
	if movingDarknessSlider then
		
		local xOffset = (mouse.X - colourGradientFrame.AbsolutePosition.X)

		xOffset = math.clamp(xOffset, 0, colourGradientFrame.AbsoluteSize.X)

		local sliderPosNew = UDim2.new(0, xOffset, colourSlider.Position.Y)
		darknessSlider.Position = sliderPosNew
	end
end)



function returnColour(percentage, gradientKeyPoints)

	local leftColour = gradientKeyPoints[1]
	local rightColour = gradientKeyPoints[#gradientKeyPoints]
	
	local lerpPercent = 0.5
	local colour = leftColour.Value


	for i = 1, #gradientKeyPoints - 1 do
		
		if gradientKeyPoints[i].Time <= percentage and gradientKeyPoints[i + 1].Time >= percentage then
			
			leftColour = gradientKeyPoints[i]
			rightColour = gradientKeyPoints[i + 1]
			
			lerpPercent = (percentage - leftColour.Time) / (rightColour.Time - leftColour.Time)
			
			colour = leftColour.Value:Lerp(rightColour.Value, lerpPercent)
			
			return colour
		end
	end
end

function updateColourPreview()
	
	local colourMinXPos = colourGradientFrame.AbsolutePosition.X
	local colourMaxXPos = colourMinXPos + colourGradientFrame.AbsoluteSize.X

	local colourXPixelSize = colourMaxXPos - colourMinXPos

	local colourSliderX = colourSlider.AbsolutePosition.X

	local colourXPos = (colourSliderX - colourMinXPos) / colourXPixelSize
	
	
	local darknessMinXPos = darknessGradientFrame.AbsolutePosition.X
	local darknessMaxXPos = darknessMinXPos + darknessGradientFrame.AbsoluteSize.X

	local darknessXPixelSize = darknessMaxXPos - darknessMinXPos

	local darknessSliderX = darknessSlider.AbsolutePosition.X

	local darknessXPos = (darknessSliderX - darknessMinXPos) / darknessXPixelSize


	local darkness = returnColour(darknessXPos, darknessGradientFrame.DarknessGradient.Color.Keypoints)
	local darknessR, darknessG, darknessB = 255 - math.floor(darkness.R * 255), 255 - math.floor(darkness.G * 255), 255 - math.floor(darkness.B * 255)
	
	
	local colour = returnColour(colourXPos, colourGradientFrame.ColourGradient.Color.Keypoints)
	local colourR, colourG, colourB = math.floor(colour.R * 255), math.floor(colour.G * 255), math.floor(colour.B * 255)
	
	local resultColour = Color3.fromRGB(colourR - darknessR, colourG - darknessG, colourB - darknessB)

	colourPreview.BackgroundColor3 = resultColour
end


colourSlider:GetPropertyChangedSignal("Position"):Connect(updateColourPreview)
darknessSlider:GetPropertyChangedSignal("Position"):Connect(updateColourPreview)
end))
