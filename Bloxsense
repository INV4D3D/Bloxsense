LocalScript116.Name = "Viewmodel"
LocalScript116.Parent = ImageButton114
table.insert(cors,sandbox(LocalScript116,function()
local button = script.Parent
local toggled = false
local TP = script.Parent.Parent.Parent.EffectsTab.ThirdPerson

function waste()
end
local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		
		while toggled == true do
			local ArmColor = script.Parent.Color.BackgroundColor3
			local GunColor = script.Parent.Color2.BackgroundColor3
			if TP.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				wait()
				waste()
			else
				wait()
				if script.Parent.Parent.DropDown.Selection.Text == ("Forcefield") then
					for _,gun in pairs(workspace.Camera:GetDescendants()) do
						if gun.Name ~= "Flash" and gun.Name ~= "Right Arm" and gun.Name ~= "Left Arm" and gun.Name ~= "Sleeve" and gun.Name ~= "Glove" and gun.Name ~= "HumanoidRootPart" then
							pcall(function() 
								gun.Material = Enum.Material.ForceField
								gun.Color = GunColor
								gun.Transparency = 0
							end)
						end				
					end
					for _,arm in pairs(workspace.Camera:GetDescendants()) do 
						if arm.Name ~= "Flash" and arm.Name ~= "Handle" and arm.Name ~= "Handle2" and arm.Name ~= "Mag" and arm.Name ~= "Mag2" and arm.Name ~= "Slide" and arm.Name ~= "Slide2" and arm.Name ~= "Barrel" and arm.Name ~= "Scope" and arm.Name ~= "Body" and arm.Name ~= "Part" and arm.Name ~= "Slide" and arm.Name ~= "Bolt" and arm.Name ~= "HumanoidRootPart" then
							pcall(function() 
								arm.Material = Enum.Material.ForceField
								arm.Color = ArmColor
								arm.Transparency = 0.5
							end)
						end				
					end
					wait()
				end
			end	
		end				
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
end

		
end	
button.Activated:Connect(onButtonActivated)
end))
