					Sky.SkyboxBk = "rbxassetid://271042516"
					Sky.SkyboxDn = "rbxassetid://271077243"
					Sky.SkyboxFt = "rbxassetid://271042556"
					Sky.SkyboxRt = "rbxassetid://271042467"
					Sky.SkyboxUp = "rbxassetid://271077958"
