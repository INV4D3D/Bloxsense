	v.CharacterAdded:connect(function()
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild('Head')
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
