LocalScript121.Name = "LocalMaterial"
LocalScript121.Parent = ImageButton119
table.insert(cors,sandbox(LocalScript121,function()
local button = script.Parent
local toggled = false
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		while toggled == true do
			wait(1/100)
			local Color = script.Parent.Color.BackgroundColor3
			if plr.Character then
				if script.Parent.Parent.DropDown2.Selection.Text == ("Glass") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetDescendants()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "HeadHB" then
							pcall(function() 
								v.Material = Enum.Material.Glass
								v.Color = Color
								v.Transparency = 0.7
								v.Mesh.TextureId = ""
							end)
						end
					end
				end

				if script.Parent.Parent.DropDown2.Selection.Text == ("Forcefield") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetDescendants()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "HeadHB" then
							pcall(function() 
								v.Material = Enum.Material.ForceField
								v.Color = Color
								v.Transparency = 0.1
							end)
						end
					end
				end

				if script.Parent.Parent.DropDown2.Selection.Text == ("Plastic") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetDescendants()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "HeadHB" then
							pcall(function() 
								v.Material = Enum.Material.Neon
								v.Color = Color
								v.Transparency = 0
								v.Mesh.TextureId = ""
							end)
						end
					end
				end
			end
		end	
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
end

		
end	
button.Activated:Connect(onButtonActivated)
end))
