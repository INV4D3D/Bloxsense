LocalScript705.Name = "Toggle"
LocalScript705.Parent = ImageButton704
table.insert(cors,sandbox(LocalScript705,function()
local button = script.Parent

local toggled = false
local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)

	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)

	end
end
button.Activated:Connect(onButtonActivated)




end))
LocalScript706.Name = "YAW"
LocalScript706.Parent = ImageButton704
table.insert(cors,sandbox(LocalScript706,function()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local cam = workspace.CurrentCamera
local spinangle = 0
local upsidedown = false
function AA(angleX, angleY)
	plr.Character.HumanoidRootPart.CFrame = CFrame.new(plr.Character.HumanoidRootPart.Position, plr.Character.HumanoidRootPart.Position + Vector3.new(cam.CFrame.lookVector.X, 0, cam.CFrame.lookVector.Z)) * CFrame.Angles(math.rad(angleX), math.rad(angleY), 0)
end

function fix()
	local bruh = false
	if upsidedown and not bruh then
		plr.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame + Vector3.new(0,2,0)
		upsidedown = false
	end
	bruh = true
end

repeat wait() until script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42)
while wait(1/((1/1+script.Parent.Parent.Background.Percent.Value))) do
	if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
		pcall(function()
			spinangle = spinangle + (1/1+script.Parent.Parent.Background.Percent.Value)
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Forward" then					
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
					AA(0, 0)
				end
			end

			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Jitter left" then
					if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
					
					AA(0, math.random(180-(script.Parent.Parent.Background2.Percent.Value),180))
					end
				end

			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Jitter right" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
					AA(0, math.random(180,180+(script.Parent.Parent.Background2.Percent.Value)))
				end
			end

			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Jitter random" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
					AA(0, math.random(180-(script.Parent.Parent.Background2.Percent.Value),180+(script.Parent.Parent.Background2.Percent.Value)))
				end
			end

			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Spin" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
					AA(0, spinangle)
				end
			end

			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Backwards" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
					AA(0, 180)
				end
			end

			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Static" then					
				game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
			end	
		end)
	end
end

end))
