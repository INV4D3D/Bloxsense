table.insert(cors,sandbox(LocalScript152,function()
local button = script.Parent
local toggled = false
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		while toggled == true do
			wait(1/100)
			local Color = script.Parent.Color2.BackgroundColor3
			if plr.Character then
				if script.Parent.Parent.DropDown2.Selection.Text == ("Plastic") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetDescendants()) do 
						if v.Name == "FakeHead" or v.Name == "LeftFoot" or v.Name == "LeftHand" or v.Name == "LeftLowerArm" or v.Name == "LeftLowerLeg" or v.Name == "LeftUpperArm" or v.Name == "LeftUpperLeg" or v.Name == "LowerTorso" or v.Name == "RightFoot" or v.Name == "RightHand" or v.Name == "RightLowerArm" or v.Name == "RightLowerLeg" or v.Name == "RightUpperArm" or v.Name == "RightUpperLeg" or v.Name == "UpperTorso" then
							pcall(function() 
								v.Material = Enum.Material.Plastic
								v.Color = Color
								v.Transparency = 0
							end)
						end
					end
				end

				if script.Parent.Parent.DropDown2.Selection.Text == ("Forcefield") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetDescendants()) do 
						if v.Name == "FakeHead" or v.Name == "LeftFoot" or v.Name == "LeftHand" or v.Name == "LeftLowerArm" or v.Name == "LeftLowerLeg" or v.Name == "LeftUpperArm" or v.Name == "LeftUpperLeg" or v.Name == "LowerTorso" or v.Name == "RightFoot" or v.Name == "RightHand" or v.Name == "RightLowerArm" or v.Name == "RightLowerLeg" or v.Name == "RightUpperArm" or v.Name == "RightUpperLeg" or v.Name == "UpperTorso" then
							pcall(function() 
								v.Material = Enum.Material.ForceField
								v.Color = Color
								v.Transparency = 0.1
							end)
						end
					end
				end

				if script.Parent.Parent.DropDown2.Selection.Text == ("Neon") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetDescendants()) do 
						if v.Name == "FakeHead" or v.Name == "LeftFoot" or v.Name == "LeftHand" or v.Name == "LeftLowerArm" or v.Name == "LeftLowerLeg" or v.Name == "LeftUpperArm" or v.Name == "LeftUpperLeg" or v.Name == "LowerTorso" or v.Name == "RightFoot" or v.Name == "RightHand" or v.Name == "RightLowerArm" or v.Name == "RightLowerLeg" or v.Name == "RightUpperArm" or v.Name == "RightUpperLeg" or v.Name == "UpperTorso" then
							pcall(function() 
								v.Material = Enum.Material.Neon
								v.Color = Color
								v.Transparency = 0
							end)
						end
					end
				end
			end
		end	
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
end

		
end	
button.Activated:Connect(onButtonActivated)
end))
