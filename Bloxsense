		local players=game:GetService("Players")
		local currentPlayer=nil
		local lplayer=players.LocalPlayer
		currentPlayer=nil
		plr=players.LocalPlayer

		function esp(v)
			for _,o in pairs(workspace[v.Name]:GetChildren()) do
