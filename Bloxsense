LocalScript155.Name = "Selected"
LocalScript155.Parent = Frame105
table.insert(cors,sandbox(LocalScript155,function()
local button = script.Parent.Icon
local IMG = script.Parent.Icon.IMG

local function onMouseEntered()
	if script.Parent.Main.Visible == false then
		IMG.ImageColor3 = Color3.fromRGB(158, 158, 158)
		local function onButtonActivated()
			if IMG.ImageColor3 ~= Color3.fromRGB(208, 208, 208) then
				IMG.ImageColor3 = Color3.fromRGB(208, 208, 208)
			end
		end
		local function onButtonActivated1()
			if IMG.ImageColor3 ~= Color3.fromRGB(208, 208, 208)then
				IMG.ImageColor3 = Color3.fromRGB(208, 208, 208)
			end
		end
		button.Activated:Connect(onButtonActivated)
		IMG.Activated:Connect(onButtonActivated1)
	end
	
end
local function onMouseLeft()
	if script.Parent.Main.Visible == false then
		IMG.ImageColor3 = Color3.fromRGB(116, 116, 116)
	end
end

     
	
button.MouseEnter:Connect(onMouseEntered)
button.MouseLeave:Connect(onMouseLeft)


end))
