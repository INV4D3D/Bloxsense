LocalScript24.Name = "BT"
LocalScript24.Parent = ImageButton22
table.insert(cors,sandbox(LocalScript24,function()
local button = script.Parent
local toggled = false
local color
local BTLEVEL = 100
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local client = getsenv(plr.PlayerGui.Client)

local BacktrackFolder = Instance.new("Folder")
BacktrackFolder.Parent = game.Workspace
BacktrackFolder.Name = "BACKTRACK TICKS"


--local mt = getrawmetatable(game)
--local oldNamecall = mt.__namecall
--setreadonly(mt, false)


local runserv = game:GetService("RunService")
runserv:BindToRenderStep("Rainbow", 1000, function()
	local hue = tick() % 5 / 5
	color = Color3.fromHSV(hue, 1, 1)
end)

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		if toggled == true then
		--[[	mt.__namecall = newcclosure(function(self, ...)
				local method = getnamecallmethod()
				local callingscript = getcallingscript()
				local args = {...}

				if not checkcaller() then
					if method == "SetPrimaryPartCFrame" and self.Name == "Arms" then								
					elseif method == "FireServer" and self.Name == "HitPart" then
						if args[1].Parent == game.Workspace["BACKTRACK TICKS"] then
							if args[1].PlayerName.Value.Character and args[1].PlayerName.Value.Character.Head ~= nil and toggled == true then
								args[1] = args[1].PlayerName.Value.Character.Head	
								pcall(function()
									wait(game.ReplicatedStorage.Weapons:FindFirstChild(tostring(client.gun)).FireRate.Value)
								end)
							end
						end
					end		
				end
				return oldNamecall(self, unpack(args))
			end)
		]]	while toggled == true do
				wait(1/math.clamp(tonumber(script.Parent.Parent.DropDown1.Selection.Text), 4, 64))
					if script.Parent.Parent.DropDown.Selection.Text == "Super low" then
						BTLEVEL = 100
					end
					if script.Parent.Parent.DropDown.Selection.Text == "Low" then
						BTLEVEL = 250
					end
					if script.Parent.Parent.DropDown.Selection.Text == "Medium" then
						BTLEVEL = 500
					end
					if script.Parent.Parent.DropDown.Selection.Text == "High" then
						BTLEVEL = 1000
					end
					if script.Parent.Parent.DropDown.Selection.Text == "Maximum" then
						BTLEVEL = 2000
					end
					for i,v in pairs(game.Players:GetPlayers()) do
						pcall(function()
						if v.Character and v.Character.Humanoid and v.Character.Humanoid.Health > 0 and v.TeamColor ~= game.Players.LocalPlayer.TeamColor then
							local trans = 0
								local BacktrackPart = Instance.new("Part")
								BacktrackPart.Name = v.Name
								BacktrackPart.Anchored = true
								BacktrackPart.CanCollide = false
								BacktrackPart.Position = v.Character.Head.Position
								BacktrackPart.Orientation = v.Character.Head.Orientation
								BacktrackPart.Size = v.Character.Head.Size - Vector3.new(-0.05, -0.05, -0.05)
								BacktrackPart.Transparency = trans
								BacktrackPart.Color = color
								BacktrackPart.Material = "SmoothPlastic"
								BacktrackPart.Parent = BacktrackFolder

								local BacktrackTag = Instance.new("ObjectValue")
								BacktrackTag.Parent = BacktrackPart
								BacktrackTag.Name = "PlayerName"
								BacktrackTag.Value = v
							
							
							
							
								local BacktrackPartA = Instance.new("Part")
								BacktrackPartA.Name = v.Name
								BacktrackPartA.Anchored = true
								BacktrackPartA.CanCollide = false
								BacktrackPartA.Position = v.Character.LeftFoot.Position
								BacktrackPartA.Orientation = v.Character.LeftFoot.Orientation
								BacktrackPartA.Size = v.Character.LeftFoot.Size - Vector3.new(-0.05, -0.05, -0.05)
								BacktrackPartA.Transparency = trans
								BacktrackPartA.Color = color
								BacktrackPartA.Material = "SmoothPlastic"
								BacktrackPartA.Parent = BacktrackFolder

								local BacktrackTag1 = Instance.new("ObjectValue")
								BacktrackTag1.Parent = BacktrackPartA
								BacktrackTag1.Name = "PlayerName"
								BacktrackTag1.Value = v
							
							
							
							
								local BacktrackPartB = Instance.new("Part")

								BacktrackPartB.Name = v.Name
								BacktrackPartB.Anchored = true
								BacktrackPartB.CanCollide = false
								BacktrackPartB.Position = v.Character.LeftHand.Position
								BacktrackPartB.Orientation = v.Character.LeftHand.Orientation
								BacktrackPartB.Size = v.Character.LeftHand.Size - Vector3.new(-0.05, -0.05, -0.05)
								BacktrackPartB.Transparency = trans
								BacktrackPartB.Color = color
								BacktrackPartB.Material = "SmoothPlastic"
								BacktrackPartB.Parent = BacktrackFolder

								local BacktrackTag2 = Instance.new("ObjectValue")
								BacktrackTag2.Parent = BacktrackPartB
								BacktrackTag2.Name = "PlayerName"
