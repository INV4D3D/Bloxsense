LocalScript1089.Name = "healthbar"
LocalScript1089.Parent = ImageButton1088
table.insert(cors,sandbox(LocalScript1089,function()
local TOGGLED = false
function round(n) 
	return math.floor(n + 0.5) 
end
local ThreadE = coroutine.create(function()
	while wait(0.05) do
		pcall(function()
			for _,v in pairs(game.Players:GetChildren()) do
				if workspace:FindFirstChild(v.Name) then
					for _,o in pairs(workspace[v.Name].HumanoidRootPart:GetChildren()) do
						if o:IsA("BillboardGui") and o.Name=="healthbaresp" then
							for _,x in pairs(o:GetChildren()) do
								pcall(function()									
									o.front.Size = UDim2.new(1, 0, round(game.Workspace[v.Name].Humanoid.Health)/100, 0)
									o.front.Position = UDim2.new(0, 0, (1-round(game.Workspace[v.Name].Humanoid.Health)/100), 0)
									o.front.BackgroundColor3 = Color3.fromHSV((round(game.Workspace[v.Name].Humanoid.Health)/255), 0.78, 1)
								end)
							end
						end
					end
				end
			end
		end)
	end
end)
coroutine.resume(ThreadE)
script.Parent.MouseButton1Click:Connect(function()
	TOGGLED = not TOGGLED
	if TOGGLED == false then
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		for _,v in pairs(game.Players:GetChildren()) do
			if workspace:FindFirstChild(v.Name) then
				for _,o in pairs(workspace[v.Name]:GetDescendants()) do
					if o.Name=="healthbaresp" then
						o:Destroy()
					end
				end
			end
		end
	else
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		local players=game:GetService("Players")
		local currentPlayer=nil
		local lplayer=players.LocalPlayer
		currentPlayer=nil
		plr=players.LocalPlayer

		function esp(v)
			for _,o in pairs(workspace[v.Name]:GetChildren()) do
				if o.Name=="HumanoidRootPart" then 
					local GUI4 = Instance.new('BillboardGui', o)
					local back = Instance.new('Frame', GUI4)
					local front = Instance.new('Frame', GUI4)

					GUI4.Adornee = o
					GUI4.Size = UDim2.new(0, 3, 7, 0)
					GUI4.StudsOffset = Vector3.new(-2.5, -0.4, 0)
					GUI4.Parent = o
					GUI4.AlwaysOnTop = true
					GUI4.Name = "healthbaresp"

					back.BackgroundColor3 = Color3.fromRGB(118, 118, 118)
					back.BorderSizePixel = 1
					back.BorderMode = Enum.BorderMode.Inset
					back.Name = "bacc"
					back.BorderColor3 = Color3.fromRGB(66, 66, 66)
					back.Position = UDim2.new(0, 0, 0, 0)
					back.Size = UDim2.new(1, 0, 1, 0)
					back.ZIndex = 0
					back.BackgroundTransparency = 0.3

					front.BorderSizePixel = 1
					front.BorderColor3 = Color3.fromRGB(66, 66, 66)
					front.Name = "front"
					front.BorderMode = Enum.BorderMode.Inset
					front.Position = UDim2.new(0, 0, 0, 0)
					front.Size = UDim2.new(1, 0, 1, 0)
					front.ZIndex = 0
					front.BackgroundColor3 = Color3.fromRGB(255, 255, 255)

					o.Parent.HumanoidRootPart.AncestryChanged:connect(function()
						GUI4:Destroy()
					end)		
				end
			end
		end

		wait(1)
		for i,v in next, game:GetService'Players':GetChildren() do
			if v~=plr then
				if workspace:FindFirstChild(v.Name) then
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end

	end
end)

for i,v in pairs(game.Players:GetChildren()) do
	v.CharacterAdded:connect(function()
		wait(1)
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild("HumanoidRootPart")
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						esp(v)
					end
				else
					esp(v)
				end
			end
		end
	end)
end

game.Players.PlayerAdded:connect(function(v)
	v.CharacterAdded:connect(function()
		wait(1)
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild("HumanoidRootPart")
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						wait(0.1)
						esp(v)
					end
				else
					wait(0.1)
					esp(v)
				end
			end
		end
	end)
end)
end))
