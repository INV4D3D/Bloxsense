LocalScript854.Name = "PITCH"
LocalScript854.Parent = ImageButton851
table.insert(cors,sandbox(LocalScript854,function()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local cam = workspace.CurrentCamera
local UserInputService = game:GetService("UserInputService")

game['Run Service'].Stepped:connect(function()
	if UserInputService:IsKeyDown(Enum.KeyCode.E) then
		
	else
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then	
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Pitch.Selection.Text == "Default" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					if game.Players.LocalPlayer.Character then
						pcall(function()
							game:GetService("ReplicatedStorage").Events.ControlTurn:FireServer("0")
						end)					
					end
				end
			end
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Pitch.Selection.Text == "Up" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					if game.Players.LocalPlayer.Character then
						pcall(function()
							game:GetService("ReplicatedStorage").Events.ControlTurn:FireServer("1")
						end)					
					end
				end
			end
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Pitch.Selection.Text == "Down" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					if game.Players.LocalPlayer.Character then
						pcall(function()
							game:GetService("ReplicatedStorage").Events.ControlTurn:FireServer("-1")
						end)					
					end
				end
			end
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Pitch.Selection.Text == "Minimal" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					if game.Players.LocalPlayer.Character then
						pcall(function()
							game:GetService("ReplicatedStorage").Events.ControlTurn:FireServer("-0.84")
						end)					
					end
				end
			end
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Pitch.Selection.Text == "Random" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					if game.Players.LocalPlayer.Character then
						pcall(function()
							game:GetService("ReplicatedStorage").Events.ControlTurn:FireServer((math.random(-1000,1000))/1000)
						end)
					end
				end
			end
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Pitch.Selection.Text == "180" then
				if game.Players.LocalPlayer.Character then
					game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
					if game.Players.LocalPlayer.Character then
						pcall(function()
							game:GetService("ReplicatedStorage").Events.ControlTurn:FireServer("2.5")
						end)
					end
				end
			end
		end	
	end
end)


			

end))
LocalScript855.Name = "YAW"
LocalScript855.Parent = ImageButton851
table.insert(cors,sandbox(LocalScript855,function()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local cam = workspace.CurrentCamera
local spinangle = 0
local roll = 0
local realangle = 0
local waittime = 0
local baseangle = 0
local yawangle = 0
local jitterangle = 0
local UserInputService = game:GetService("UserInputService")

function AA(angleX, angleY)
	plr.Character.HumanoidRootPart.CFrame = CFrame.new(plr.Character.HumanoidRootPart.Position, plr.Character.HumanoidRootPart.Position + Vector3.new(cam.CFrame.lookVector.X, 0, cam.CFrame.lookVector.Z)) * CFrame.Angles(math.rad(angleX), math.rad(angleY), 0)
end

--waittime for fakelag animation lagginess thingo
spawn(function()
	while wait() do
		if script.Parent.Parent.Parent.FakeLagTab.Fakelag.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			waittime = math.clamp((2.5*script.Parent.Parent.Parent.FakeLagTab.LIM.Percent.Value)/500, 0, math.huge)
			print(waittime)
		else
			waittime = 1/250
		end	
	end
end)


--yaw
local yawthread = coroutine.create(function()
	while wait(waittime) do
		spinangle = spinangle + script.Parent.Parent.Yawmod.Percent.Value
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "180" then					
			yawangle = 180+((script.Parent.Parent.Yawmod.Percent.Value*3.6)-180)
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Spin" then					
			yawangle = spinangle + ((script.Parent.Parent.Yawmod.Percent.Value*3.6)-180)
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Static" then	
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "180 Z" then					
			yawangle = 150+((script.Parent.Parent.Yawmod.Percent.Value*3.6)-180)
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Crosshair" then					
			yawangle = 360+((script.Parent.Parent.Yawmod.Percent.Value*3.6)-180)
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.Yaw.Selection.Text == "Off" then					
			yawangle = 0
		end
	end
end)


--jitter
local jitterthread = coroutine.create(function()
	while wait(waittime/2) do
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.YawJitter.Selection.Text == "Off" then					
			jitterangle = 0
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.YawJitter.Selection.Text == "Offset" then					
			jitterangle = 0 + ((script.Parent.Parent.Yawjittermod.Percent.Value*3.6)-180)
			wait(waittime)
			jitterangle = 0
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.YawJitter.Selection.Text == "Center" then					
			jitterangle = 0 + ((script.Parent.Parent.Yawjittermod.Percent.Value*3.6)-180)
			wait(waittime)
			jitterangle = 0 - ((script.Parent.Parent.Yawjittermod.Percent.Value*3.6)-540)
		end
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) and script.Parent.Parent.YawJitter.Selection.Text == "Random" then	
			jitterangle = math.random(0 + ((script.Parent.Parent.Yawjittermod.Percent.Value*3.6)-180),0 - ((script.Parent.Parent.Yawjittermod.Percent.Value*3.6)-540))
			wait(waittime)
		end
	end
end)

coroutine.resume(yawthread)
coroutine.resume(jitterthread)

game["Run Service"].RenderStepped:Connect(function()
	realangle = yawangle + baseangle + jitterangle 
	if script.Parent.Parent.Parent.OtherTab["Upside down aa"].BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
		roll = 180
		plr.Character.Humanoid.HipHeight = 3.8
	else
		roll = 0
		plr.Character.Humanoid.HipHeight = 2
	end
	if UserInputService:IsKeyDown(Enum.KeyCode.E) then
		realangle = 0
	end
	pcall(function()
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			plr.Character.Humanoid.AutoRotate = false
			if script.Parent.Parent.Pitch.Selection.Text == "Default" then
				AA(roll, realangle)	
			end
			if script.Parent.Parent.Pitch.Selection.Text == "Minimal" then
				AA(roll, realangle)	
			end
			if script.Parent.Parent.Pitch.Selection.Text == "Up" then
				AA(roll, realangle+180)		
			end
			if script.Parent.Parent.Pitch.Selection.Text == "Down" then
				AA(roll, realangle)		
			end
			if script.Parent.Parent.Pitch.Selection.Text == "Random" then
				AA(roll, realangle)	
			end
			if script.Parent.Parent.Pitch.Selection.Text == "180" then
				AA(roll, realangle+180)	
			end
			if script.Parent.Parent.Pitch.Selection.Text == "Off" then
				AA(roll, realangle)		
			end		
		end	
	end)		
end)
end))
