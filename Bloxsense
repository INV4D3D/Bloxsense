ImageButton1041.BackgroundColor3 = Color3.new(0.290196, 0.290196, 0.290196)
ImageButton1041.BorderColor = BrickColor.new("Really black")
ImageButton1041.BorderColor3 = Color3.new(0.101961, 0.101961, 0.101961)
ImageButton1041.BorderSizePixel = 2
ImageButton1041.Image = "rbxassetid://5761429802"
ImageButton1041.ImageColor3 = Color3.new(0.745098, 0.745098, 0.745098)
ImageButton1041.ImageTransparency = 0.5
LocalScript1042.Name = "BulletTracers"
LocalScript1042.Parent = ImageButton1041
table.insert(cors,sandbox(LocalScript1042,function()
local button = script.Parent
local toggled = false
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

function createBeam(v1, v2)
	local b1 = Instance.new("Part", workspace["Ray_Ignore"])
	b1.Size = Vector3.new(0.0001,0.0001,0.0001)
	b1.Transparency = 1
	b1.CanCollide = false
	b1.CFrame = CFrame.new(v1)
	b1.Anchored = true
	local a1 = Instance.new("Attachment", b1)
	local b2 = Instance.new("Part", workspace["Ray_Ignore"])
	b2.Size = Vector3.new(0.0001,0.0001,0.0001)
	b2.Transparency = 1
	b2.CanCollide = false
	b2.CFrame = CFrame.new(v2)
	b2.Anchored = true
	local a2 = Instance.new("Attachment", b2)
	local b = Instance.new("Beam", b1)
	local color = script.Parent.Color.BackgroundColor3
	b.FaceCamera = true
	b.Attachment0 = a1
	b.Attachment1 = a2
	b.LightEmission = 1
	b.LightInfluence = 0
	b.Color = ColorSequence.new(color, color)
	b.Width0 = 0.1
	b.Width1 = 0.1
	b.Transparency = NumberSequence.new(0,0)
	spawn(function()
		wait(0.5)
		while wait() do
			for i = 0,1,0.05 do
				wait()
				b.Transparency = NumberSequence.new(i,i)
			end
			b1:Destroy()
			b2:Destroy()
		end
	end)
end

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		
			local TRACERTABLE = getrawmetatable(game)
			local oldNamecall = TRACERTABLE.__namecall

			setreadonly(TRACERTABLE, false)

			TRACERTABLE.__namecall = newcclosure(function(self, ...)
				local method = getnamecallmethod()
				local callingscript = getcallingscript()
				local args = {...}

				if not checkcaller() then
				if method == "SetPrimaryPartCFrame" and self.Name == "Arms" then
					elseif method == "FireServer" and self.Name == "HitPart" and toggled then
						pcall(function()
							local beam = createBeam(game.Players.LocalPlayer.Character.Head.CFrame.p, args[2])	
						end)
					end
				end
				return oldNamecall(self, unpack(args))
			end)	
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	end
end

button.Activated:Connect(onButtonActivated)
end))
