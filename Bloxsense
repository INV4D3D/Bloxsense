					GUI2.StudsOffset = Vector3.new(0, 14.1, 0)
					GUI2.Parent = o
					GUI2.AlwaysOnTop = true
					GUI2.Name = "nameesp"

					nameesp.BackgroundTransparency = 1
					nameesp.Position = UDim2.new(0, 0, 0, -2)
					nameesp.Size = UDim2.new(1, 0, 1, 0)
					nameesp.Font = Enum.Font.SourceSans
					nameesp.TextScaled = true
					nameesp.TextSize = 15
					nameesp.RichText = true
					nameesp.TextStrokeColor3 = Color3.fromRGB(94, 94, 94)
					nameesp.TextStrokeTransparency = 0.7
					nameesp.TextXAlignment = Enum.TextXAlignment.Center
					nameesp.TextYAlignment = Enum.TextYAlignment.Bottom
					
					constraint.MaxTextSize = 14
					constraint.MinTextSize = 14
					o.Parent.HumanoidRootPart.AncestryChanged:connect(function()
						GUI2:Destroy()
					end)	
				end
			end
		end

		wait(1)
		for i,v in next, game:GetService'Players':GetChildren() do
			if v~=plr then
				if workspace:FindFirstChild(v.Name) then
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end

	end
end)

for i,v in pairs(game.Players:GetChildren()) do
	v.CharacterAdded:connect(function()
		wait(1)
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild("HumanoidRootPart")
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						esp(v)
					end
				else
					esp(v)
				end
			end
		end
	end)
end

