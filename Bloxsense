							BoxHandleAdornment9:Destroy()
							BoxHandleAdornment10:Destroy()
							BoxHandleAdornment11:Destroy()
							BoxHandleAdornment12:Destroy()
						end)	
					end
				end
			end
		end

		for i,v in next, game:GetService'Players':GetChildren() do
			if v~=plr then
				if workspace:FindFirstChild(v.Name) then
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
							wait(0.3)
							esp(v)
						end
					else
						wait(0.3)
						esp(v)
					end
				end
			end
		end
		while TOGGLED do
			wait()
			local color = script.Parent.Color.BackgroundColor3
			for	i,v in pairs(game.CoreGui.bones:GetDescendants()) do
				if v:IsA("BoxHandleAdornment") then
					v.Color3 = color
				end
			end
		end
	end
end)

