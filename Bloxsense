ImageButton666.Size = UDim2.new(0, 12, 0, 12)
ImageButton666.BackgroundColor = BrickColor.new("Dark taupe")
ImageButton666.BackgroundColor3 = Color3.new(0.290196, 0.290196, 0.290196)
ImageButton666.BorderColor3 = Color3.new(0.137255, 0.137255, 0.137255)
ImageButton666.ZIndex = 999
ImageButton666.Image = "rbxassetid://5761429802"
ImageButton666.ImageTransparency = 0.25
LocalScript667.Name = "FakeLag"
LocalScript667.Parent = ImageButton666
table.insert(cors,sandbox(LocalScript667,function()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local body = plr.Character
local player = game.Players.LocalPlayer

function packetchoke(amount)
	local netSettings = settings():GetService('NetworkSettings')
	netSettings.IncomingReplicationLag = amount
	script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.Visible = true	
end
	
function stoppacketchoke()
	local netSettings = settings():GetService('NetworkSettings')
	netSettings.IncomingReplicationLag = 0
	script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.Visible = false
end

repeat wait() until script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42)
if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
	while wait() do
		if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then					
			if script.Parent.Parent.Frequency.Selection.Text == "Low" then
				packetchoke(tonumber(script.Parent.Parent.Background.Bar.Button.ValueText.Text))
				wait(0.1)
				stoppacketchoke()
				wait(0.5)
			end
			
			if script.Parent.Parent.Frequency.Selection.Text == "Medium" then
				packetchoke(tonumber(script.Parent.Parent.Background.Bar.Button.ValueText.Text))
				wait(0.25)
				stoppacketchoke()
				wait(1)
			end
			if script.Parent.Parent.Frequency.Selection.Text == "High" then
				packetchoke(tonumber(script.Parent.Parent.Background.Bar.Button.ValueText.Text))
				wait(0.5)
				stoppacketchoke()
				wait(2)
			end
		end
	end
end





end))
LocalScript668.Name = "Toggle"
LocalScript668.Parent = ImageButton666
table.insert(cors,sandbox(LocalScript668,function()
local button = script.Parent

local toggled = false
local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)

	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)

	end
end
button.Activated:Connect(onButtonActivated)




end))
