table.insert(cors,sandbox(LocalScript1168,function()
local button = script.Parent
local toggled = false
local TP = script.Parent.Parent.Parent.EffectsTab.ThirdPerson

local function color3ToVector3(c3)
	return Vector3.new(c3.r, c3.g, c3.b)
end

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		game.Workspace.CurrentCamera.ChildAdded:Connect(function(ViewmodelFolder)
			local ArmsTransparency = script.Parent.Parent.ArmTrans.Percent.Value/100
			if ViewmodelFolder.Name == 'Arms' and toggled and script.Parent.Parent.DropDown.Selection.Text == "Forcefield" and TP.TP.Value == false then
				local ArmsColorsColor = script.Parent.Color.BackgroundColor3
				
				local ArmsColorsColor2 = script.Parent.Color2.BackgroundColor3
				for i,v in pairs(ViewmodelFolder:GetChildren()) do
					if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
						-- Arms Pointer
						local RightArm = v["Right Arm"]
						local LeftArm = v["Left Arm"]
						-- Gloves Pointer
						local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
						local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
						-- Sleeves Pointer
						local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
						local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

						RightArm.Transparency = ArmsTransparency
						RightArm.Reflectance = 0
						RightArm.Color = ArmsColorsColor
						RightArm.Material = "ForceField"

						LeftArm.Transparency = ArmsTransparency
						LeftArm.Reflectance = 0
						LeftArm.Color = ArmsColorsColor
						LeftArm.Material = "ForceField"

						RightGlove.Transparency = ArmsTransparency
						RightGlove.Reflectance = 0
						RightGlove.Color = ArmsColorsColor2
						RightGlove.Material = "ForceField"
						RightGlove.Mesh.VertexColor = color3ToVector3(ArmsColorsColor2)

						LeftGlove.Transparency = ArmsTransparency
						LeftGlove.Reflectance = 0
						LeftGlove.Color = ArmsColorsColor2
						LeftGlove.Material = "ForceField"
						LeftGlove.Mesh.VertexColor = color3ToVector3(ArmsColorsColor2)

						if RightSleeve and LeftSleeve then

							RightSleeve.Transparency = ArmsTransparency
							RightSleeve.Reflectance = 0
							RightSleeve.Color = ArmsColorsColor2
							RightSleeve.Material = "ForceField"
							RightSleeve.Mesh.VertexColor = color3ToVector3(ArmsColorsColor2)
								
							LeftSleeve.Transparency = ArmsTransparency
							LeftSleeve.Reflectance = 0
							LeftSleeve.Color = ArmsColorsColor2
							LeftSleeve.Material = "ForceField"
							LeftSleeve.Mesh.VertexColor = color3ToVector3(ArmsColorsColor2)

						end
					end
				end
			end
			if ViewmodelFolder.Name == 'Arms' and toggled and script.Parent.Parent.DropDown.Selection.Text == "Glass" and TP.TP.Value == false then
				local ArmsColorsColor = script.Parent.Color.BackgroundColor3
						for i,v in pairs(ViewmodelFolder:GetChildren()) do
							if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
								-- Arms Pointer
								local RightArm = v["Right Arm"]
								local LeftArm = v["Left Arm"]
								-- Gloves Pointer
								local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
								local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
								-- Sleeves Pointer
								local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
								local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

								RightArm.Transparency = ArmsTransparency
								RightArm.Reflectance = 0
								RightArm.Mesh.TextureId = ""
								RightArm.Color = ArmsColorsColor
								RightArm.Material = "Glass"

								LeftArm.Transparency = ArmsTransparency
								LeftArm.Reflectance = 0
								LeftArm.Mesh.TextureId = ""
								LeftArm.Color = ArmsColorsColor
								LeftArm.Material = "Glass"
						
								RightGlove.Transparency = ArmsTransparency
								RightGlove.Reflectance = 0
								RightGlove.Mesh.TextureId = ""
								RightGlove.Color = ArmsColorsColor
								RightGlove.Material = "Glass"
									
								LeftGlove.Transparency = ArmsTransparency
								LeftGlove.Reflectance = 0
								LeftGlove.Mesh.TextureId = ""
								LeftGlove.Color = ArmsColorsColor
								LeftGlove.Material = "Glass"

								if RightSleeve and LeftSleeve then

									RightSleeve.Transparency = ArmsTransparency
									RightSleeve.Reflectance = 0
									RightSleeve.Mesh.TextureId = ""
									RightSleeve.Color = ArmsColorsColor
									RightSleeve.Material = "Glass"
							
									LeftSleeve.Transparency = ArmsTransparency
									LeftSleeve.Reflectance = 0
									LeftSleeve.Mesh.TextureId = ""
									LeftSleeve.Color = ArmsColorsColor
									LeftSleeve.Material = "Glass"

								end
							end
						end
					end
					if ViewmodelFolder.Name == 'Arms' and toggled and script.Parent.Parent.DropDown.Selection.Text == "Plastic" and TP.TP.Value == false then
						local ArmsColorsColor = script.Parent.Color.BackgroundColor3
						for i,v in pairs(ViewmodelFolder:GetChildren()) do
							if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
						-- Arms Pointer
