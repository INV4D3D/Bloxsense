table.insert(cors,sandbox(LocalScript398,function()
local button = script.Parent
local colorpicker = script.Parent.Frame
local toggled = false
local function onButtonActivated()
	if toggled == false then
		toggled = true
		colorpicker.Visible = true
	else
		toggled = false
		colorpicker.Visible = false
