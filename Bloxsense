LocalScript1155.Name = "OuterChams"
LocalScript1155.Parent = ImageButton1154
table.insert(cors,sandbox(LocalScript1155,function()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local button = script.Parent
local espFolder = Instance.new("Folder")
syn.protect_gui(espFolder)
espFolder.Parent = game.CoreGui
espFolder.Name = "outer"
local pulsetrans = 0
local TOGGLED = false
spawn(function()
	while wait() do
		for i = 0, 2, 0.05 do
			wait(0.001)
			pulsetrans = math.abs(-(i-1))
		end	
	end	
end)

script.Parent.MouseButton1Click:Connect(function()
	TOGGLED = not TOGGLED
	if TOGGLED == false then
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		for	i,v in pairs(game.CoreGui.outer:GetDescendants())  do
			if v:IsA("BoxHandleAdornment") then
				v:Destroy()
			end
		end
	else
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		local players=game:GetService("Players")
		local currentPlayer=nil
		local lplayer=players.LocalPlayer
		currentPlayer=nil
		plr=players.LocalPlayer

		function esp(plr)
			pcall(function()
				if plr.Character then
					for i,v in next,plr.Character:GetChildren() do
						if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "Gun" then
							local chamThing = Instance.new("BoxHandleAdornment",espFolder)
							chamThing.Size = v.Size + Vector3.new(0.3, 0.3, 0.3)
							chamThing.Transparency = 0
							chamThing.ZIndex = -2
							chamThing.AlwaysOnTop = true
							chamThing.Visible = true
							chamThing.Adornee = v
							plr.Character.HumanoidRootPart.AncestryChanged:connect(function()
								chamThing:Destroy()
							end)
						end
					end
				end
			end)
		end

		for i,v in next, game:GetService'Players':GetChildren() do
			if v~=plr then
				if workspace:FindFirstChild(v.Name) then
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Parent.PlayerESPTab.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
							wait()
							esp(v)
						end
					else
						wait()
						esp(v)
					end
				end
			end
		end
		while TOGGLED do
			wait(0.1)
			local color = script.Parent.Color.BackgroundColor3
			for i,v in next,espFolder:GetChildren() do
				if v:IsA("BoxHandleAdornment") then
					v.Color3 = color
					if script.Parent.Parent.DropDown4.Selection.Text == "Pulse" then
						v.Transparency = pulsetrans
					end
					if script.Parent.Parent.DropDown4.Selection.Text == "Custom" then
						v.Transparency = script.Parent.Parent.ChamTrans.Percent.Value/100
					end
				end
			end
		end
	end
end)

for i,v in pairs(game.Players:GetChildren()) do
	v.CharacterAdded:connect(function()
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild('Head')
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Parent.PlayerESPTab.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						wait()
						esp(v)
					end
				else
					wait()
					esp(v)
				end
			end
		end
	end)
end

game.Players.PlayerAdded:connect(function(v)
	v.CharacterAdded:connect(function()
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild('Head')
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Parent.PlayerESPTab.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						wait()
						esp(v)
					end
				else
					wait()
					esp(v)
				end
			end
		end
	end)
end)
end))
