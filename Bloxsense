pcall(function()
	while wait() do
		--	local s = script 
		--	for _, v in pairs( script.Parent:GetDescendants() ) do 
		--		if v:IsA("TextButton") and v.Name ~= "Template" then s:Clone().Parent = v
		--		end 
		--	end
