LocalScript718.Name = "smg trigbot"
LocalScript718.Parent = ImageButton717
table.insert(cors,sandbox(LocalScript718,function()
local button = script.Parent
local toggled = false
local Holding = false
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()
local UserInputService = game:GetService("UserInputService")

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		local gun = game.Players.LocalPlayer.Character.EquippedTool.Value
		if gun==("P90") or gun==("MP9") or gun==("MP7") or gun==("Bizon") or gun==("MAC10") or gun==("UMP") or gun==("UMP45") then
			UserInputService.InputBegan:connect(function(key)
				if key.KeyCode == Enum.KeyCode.V and toggled then
					Holding = true
					while Holding do wait()
						if Mouse.Target and Players:FindFirstChild(Mouse.Target.Parent.Name) then
							local HitPlayer = Players:FindFirstChild(Mouse.Target.Parent.Name)
							if HitPlayer.Team ~= LocalPlayer.Team then
								if script.Parent.Parent.Delay.Percent.Value * 3 > 0 then wait(script.Parent.Parent.Delay.Percent.Value * 3) end
								mouse1press(); wait(); mouse1release()
							end
						end
						if not toggled then break end
					end
				end
			end)

			UserInputService.InputEnded:connect(function(key)
				if key.KeyCode == Enum.KeyCode.V and toggled then
					Holding = false
				end
			end)
		end

		UserInputService.InputEnded:connect(function(key)
			if Enum.KeyCode.V and toggled then
				Holding = false
			end
		end)

	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	end
end

button.Activated:Connect(onButtonActivated)

end))
