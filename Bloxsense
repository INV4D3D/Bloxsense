LocalScript1070.Name = "weaponesp"
LocalScript1070.Parent = ImageButton1069
table.insert(cors,sandbox(LocalScript1070,function()
local TOGGLED = false
local ThreadG = coroutine.create(function()
	while wait(0.05) do
		pcall(function()
			for _,v in pairs(game.Players:GetChildren()) do
				if workspace:FindFirstChild(v.Name) then
					for _,o in pairs(workspace[v.Name].HumanoidRootPart:GetChildren()) do
						if o:IsA("BillboardGui") and o.Name=="weaponesp" then
							for _,x in pairs(o:GetChildren()) do
								pcall(function()
									local Color = script.Parent.Color.BackgroundColor3
									x.Text = game.Workspace[v.Name].EquippedTool.Value
									x.TextColor3 = Color
								end)
							end
						end
					end
				end
			end
		end)
	end
end)
coroutine.resume(ThreadG)
script.Parent.MouseButton1Click:Connect(function()
	TOGGLED = not TOGGLED
	if TOGGLED == false then
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		for _,v in pairs(game.Players:GetChildren()) do
			if workspace:FindFirstChild(v.Name) then
				for _,o in pairs(workspace[v.Name]:GetDescendants()) do
					if o.Name=="weaponesp" then
						o:Destroy()
					end
				end
			end
		end
	else
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		local players=game:GetService("Players")
		local currentPlayer=nil
		local lplayer=players.LocalPlayer
		currentPlayer=nil
		plr=players.LocalPlayer

		function esp(v)
			for _,o in pairs(workspace[v.Name]:GetChildren()) do
				if o.Name=="HumanoidRootPart" then 
					local GUI5 = Instance.new('BillboardGui', o)
					local weaponesp = Instance.new('TextLabel', GUI5)
					local constraint = Instance.new("UITextSizeConstraint", weaponesp)
					
					GUI5.Adornee = o
					GUI5.Size = UDim2.new(100, 0, 22, 0)
					GUI5.StudsOffset = Vector3.new(0, -15.1, 0)
					GUI5.Parent = o
					GUI5.AlwaysOnTop = true
					GUI5.Name = "weaponesp"

					weaponesp.BackgroundTransparency = 1
					weaponesp.Position = UDim2.new(0, 0, 0, 0)
					weaponesp.Size = UDim2.new(1, 0, 1, 0)
					weaponesp.Font = Enum.Font.SourceSans
					weaponesp.TextScaled = true
					weaponesp.TextSize = 15
					weaponesp.RichText = true
					weaponesp.TextStrokeColor3 = Color3.fromRGB(94, 94, 94)
					weaponesp.TextStrokeTransparency = 0.7
					weaponesp.TextXAlignment = Enum.TextXAlignment.Center
					weaponesp.TextYAlignment = Enum.TextYAlignment.Top
					
					constraint.MaxTextSize = 14
					constraint.MinTextSize = 14
					o.Parent.HumanoidRootPart.AncestryChanged:connect(function()
						GUI5:Destroy()
					end)
				end
			end
		end

		wait(1)
		for i,v in next, game:GetService'Players':GetChildren() do
			if v~=plr then
				if workspace:FindFirstChild(v.Name) then
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end

	end
end)

for i,v in pairs(game.Players:GetChildren()) do
	v.CharacterAdded:connect(function()
		wait(1)
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild("HumanoidRootPart")
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						esp(v)
					end
				else
					esp(v)
				end
			end
		end
	end)
end

game.Players.PlayerAdded:connect(function(v)
	v.CharacterAdded:connect(function()
		wait(1)
		if v~=plr then
			if script.Parent.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
				v.Character:WaitForChild("HumanoidRootPart")
				if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
					if script.Parent.Parent.Teammates.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
						esp(v)
					end
				else
					esp(v)
				end
			end
		end
	end)
end)
end))
