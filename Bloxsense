ImageButton122.Name = "Hands"
ImageButton122.Parent = Frame86
ImageButton122.Position = UDim2.new(0.0919999927, 0, 0.349887371, 0)
ImageButton122.Size = UDim2.new(0, 12, 0, 12)
ImageButton122.BackgroundColor = BrickColor.new("Dark taupe")
ImageButton122.BackgroundColor3 = Color3.new(0.290196, 0.290196, 0.290196)
