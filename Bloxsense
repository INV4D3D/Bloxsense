ImageButton851.BackgroundColor = BrickColor.new("Dark taupe")
ImageButton851.BackgroundColor3 = Color3.new(0.290196, 0.290196, 0.290196)
ImageButton851.BorderColor3 = Color3.new(0.137255, 0.137255, 0.137255)
ImageButton851.ZIndex = 999
ImageButton851.Image = "rbxassetid://5761429802"
ImageButton851.ImageTransparency = 0.25
LocalScript852.Name = "Toggle"
LocalScript852.Parent = ImageButton851
table.insert(cors,sandbox(LocalScript852,function()
local button = script.Parent
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local CurrentCamera = game.Workspace.CurrentCamera
local runserv = game:GetService("RunService")
local toggled = false

spawn(function()
	runserv.Stepped:Connect(function()
		if toggled == true then
			pcall(function()
				local ReplicateCamera = game.ReplicatedStorage.Events.ReplicateCamera
				plr.CameraCF.Value = plr.Character.Head.CFrame
				ReplicateCamera:FireServer(plr.Character.Head.CFrame)	
			end)
		end
	end)
end)


local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)

	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)

	end
end
button.Activated:Connect(onButtonActivated)




end))
