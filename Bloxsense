TextLabel58.TextXAlignment = Enum.TextXAlignment.Left
LocalScript59.Name = "Ragebot"
LocalScript59.Parent = ImageButton57
table.insert(cors,sandbox(LocalScript59,function()
-- Services
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local repstor = game:GetService("ReplicatedStorage")
local runserv = game:GetService("RunService")


-- Variables
local cam = workspace.CurrentCamera
local client = getsenv(plr.PlayerGui.Client)


--Random
local rage = false
local button = script.Parent


--Prevents rapidfire
function coolDown()
	shootCooldown = true
	if game.ReplicatedStorage.Weapons:FindFirstChild(tostring(client.gun)) then
		wait(game.ReplicatedStorage.Weapons:FindFirstChild(tostring(client.gun)).FireRate.Value)
		shootCooldown = false
	end
end

--Firebullet
function fireBullet()
	if not shootCooldown then
		game:GetService("NetworkClient"):SetOutgoingKBPSLimit(9e9)
		client.firebullet()
		if script.Parent.Parent.Parent.Parent.Parent.Visual.Main.OtherESPTab.Tracers.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			local beam = createBeam(game.Players.LocalPlayer.Character.Head.CFrame.p, target.CFrame.p)
		end
		coolDown()
	end
end


--kevlar
function kevlardamage(target)
	local pen = 1
	if target:FindFirstChild("Kevlar") then
		if string.find(target.Name, "Head") then
			if target:FindFirstChild("Helmet") then
				pen = pen / 100 * client.gun.ArmorPenetration.Value
			end
		else
			pen = pen / 100 * client.gun.ArmorPenetration.Value
		end
		return pen
	end
end


--Boosted damage
function Hitpart(target, hitbox, damage)
	if not shootCooldown then
		local Remote = game.ReplicatedStorage.Events['HitPart']
		local Arguments = {
			[1] = target.Character:FindFirstChild(hitbox),
			[2] = target.Character:FindFirstChild(hitbox).CFrame.p,
			[3] = workspace[game.Players.LocalPlayer.Name].EquippedTool.Value,
			[4] = damage,
			[5] = workspace[game.Players.LocalPlayer.Name].Gun,
			[8] = 1,
			[9] = false,
			[10] = false,
			[11] = Vector3.new(),
			[12] = 100,
			[13] = Vector3.new()
		}
		Remote:FireServer(unpack(Arguments))
		coolDown()	
	end	
end


--Doubletap
function Doubletap()
	if not shootCooldown then
		client.firebullet()
		if script.Parent.Parent.Parent.Parent.Parent.Visual.Main.OtherESPTab.Tracers.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			local beam = createBeam(game.Players.LocalPlayer.Character.Head.CFrame.p, target.CFrame.p)				
		end
		client.firebullet()
		if script.Parent.Parent.Parent.Parent.Parent.Visual.Main.OtherESPTab.Tracers.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			local beam = createBeam(game.Players.LocalPlayer.Character.Head.CFrame.p, target.CFrame.p)				
		end
		coolDown()
	end
end

function createBeam(v1, v2)
	local b1 = Instance.new("Part", workspace["Ray_Ignore"])
	b1.Size = Vector3.new(0.0001,0.0001,0.0001)
	b1.Transparency = 1
	b1.CanCollide = false
	b1.CFrame = CFrame.new(v1)
	b1.Anchored = true
	local a1 = Instance.new("Attachment", b1)
	local b2 = Instance.new("Part", workspace["Ray_Ignore"])
	b2.Size = Vector3.new(0.0001,0.0001,0.0001)
	b2.Transparency = 1
	b2.CanCollide = false
	b2.CFrame = CFrame.new(v2)
	b2.Anchored = true
	local a2 = Instance.new("Attachment", b2)
	local b = Instance.new("Beam", b1)
	local color = script.Parent.Parent.Parent.Parent.Parent.Visual.Main.OtherESPTab.Tracers.Color.BackgroundColor3
	b.FaceCamera = true
	b.Attachment0 = a1
	b.Attachment1 = a2
	b.LightEmission = 1
	b.LightInfluence = 0
	b.Color = ColorSequence.new(color, color)
	b.Width0 = 0.1
	b.Width1 = 0.1
	b.Transparency = NumberSequence.new(0,0)
	spawn(function()
		wait(0.5)
		while wait() do
			for i = 0,1,0.05 do
				wait()
				b.Transparency = NumberSequence.new(i,i)
			end
			b1:Destroy()
			b2:Destroy()
		end
	end)
end

-- Fire sequence
function FireBulletSequence(hitbox, x)
	if x.Character:FindFirstChildOfClass("ForceField") then
	else
		target = hitbox								
		script.Firing.Value = true
		if script.Parent.Parent.Hitpart.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			Hitpart(x, hitbox, script.Parent.Parent.Background1.Percent.Value)
		end
		if script.Parent.Parent.Parent.OtherTab.DT.BackgroundColor3 == Color3.fromRGB(181, 255, 42) then
			Doubletap()
		end
		fireBullet()
		script.Firing.Value = false	
	end
end

--autowall
function SimulateShot(target, hitbox, rangemodifier, dmgmodifier)
	local hitList = {
		cam,
		plr.Character,
		game.Workspace.Debris,
		game.Workspace.Ray_Ignore,
		game.Workspace["BACKTRACK TICKS"],
		game.Workspace.Map:WaitForChild("Clips"),
		game.Workspace.Map:WaitForChild("SpawnPoints"),
	}
	local gun = client.gun
	local gunPen,gunRange,gunDMG
	if gun:FindFirstChild("Penetration") then
		gunPen = gun.Penetration.Value * 0.01
	end
	if gun:FindFirstChild("Range") then
		gunRange = gun.Range.Value * rangemodifier
	end
	if gun:FindFirstChild("DMG") then
		gunDMG = gun.DMG.Value * dmgmodifier
	end
	local direction = CFrame.new(cam.CFrame.p, target.Character[hitbox].Position).lookVector.unit * gunRange * 0.0694
	local rayCasted = Ray.new(cam.CFrame.p, direction)
	local hitPart, hitPos = workspace:FindPartOnRayWithIgnoreList(rayCasted, hitList, false, true)
	local partPenetrated = 0
	local limit = 0
	local partHit, posHit, normHit
	local partModifier = 1
	local damageModifier = 1
	repeat
		partHit, posHit, normHit = workspace:FindPartOnRayWithIgnoreList(rayCasted, hitList, false, true)
		if partHit and partHit.Parent then
			partModifier = 1
			if partHit.Material == Enum.Material.DiamondPlate then
				partModifier = 3
			elseif partHit.Material == Enum.Material.CorrodedMetal or partHit.Material == Enum.Material.Metal or partHit.Material == Enum.Material.Concrete or partHit.Material == Enum.Material.Brick then
				partModifier = 2
			elseif partHit.Name == "Grate" or partHit.Material == Enum.Material.Wood or partHit.Material == Enum.Material.WoodPlanks then
				partModifier = 0.1
			elseif partHit.Transparency == 1 or partHit.CanCollide == false or partHit.Name == "Glass" or partHit.Name == "Cardboard" or partHit:IsDescendantOf(game.Workspace.Ray_Ignore) or partHit:IsDescendantOf(game.Workspace.Debris) then
				partModifier = 0
			elseif partHit.Name == "nowallbang" then
				partModifier = 100
			elseif partHit:FindFirstChild("PartModifier") then
				partModifier = partHit.PartModifier.Value
			end
			local fakeHit, fakePos = workspace:FindPartOnRayWithWhitelist(Ray.new(posHit + direction * 1, direction * -2), {partHit}, true)
			local penDistance = (fakePos - posHit).magnitude
			penDistance = penDistance * partModifier
			limit = math.min(gunPen, limit + penDistance)
			local wallbang = false
			if partPenetrated >= 1 then
				wallbang = true 
			end
			if partHit and partHit.Parent and partHit.Parent.Name == "Hitboxes" or partHit and partHit.Parent.ClassName == "Accessory" or partHit and partHit.Parent.ClassName == "Hat" and partHit.Parent.Name ~= "Door" and partHit.Parent:FindFirstChild("Hostage") == nil then
			else
				if partHit and partHit:IsDescendantOf(target.Character) and partHit.Transparency < 1 or partHit.Name == hitbox then
					return partHit, posHit, damageModifier, wallbang
				end
			end
			damageModifier = 1 - limit / gunPen
			if partModifier > 0 then
				partPenetrated = partPenetrated + 1
			end
			if partHit and partHit.Parent and partHit.Parent.Name == "Hitboxes" or partHit and partHit.Parent and partHit.Parent.Parent and partHit.Parent.Parent:FindFirstChild("Humanoid2") or partHit and partHit.Parent and partHit.Parent:FindFirstChild("Humanoid2") or partHit and partHit.Parent and partHit.Parent:FindFirstChild("Humanoid") and (1 > partHit.Transparency or partHit.Name == hitbox) and partHit.Parent:IsA("Model") then
				table.insert(hitList, partHit.Parent)
			else
				table.insert(hitList, partHit)
			end
		end
	until partHit == nil or partHit.Parent == target.Character or limit >= gunPen or 0 >= damageModifier or partPenetrated >= 4
end

-- Metatable
local mt = getrawmetatable(game)
setreadonly(mt, false)
local nc = mt.__namecall

mt.__namecall = newcclosure(
	function(self,...)
		local args = {...}
		local namecall = getnamecallmethod()
		if namecall == "FindPartOnRayWithIgnoreList" then
			if target and plr.Character and rage then
				args[1] = Ray.new(workspace.CurrentCamera.CFrame.p, (target.CFrame.p - workspace.CurrentCamera.CFrame.p).unit * 500)
			end
		end

		return nc(self,unpack(args))
	end
)

local HeadThread = coroutine.create(function()
	while true do
		wait(math.random(1, 2)/150)
		pcall(function()
			if rage and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				for i,v in next,plrs:GetPlayers() do
					if v and v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.TeamColor ~= plr.TeamColor then
						local mindamage = script.Parent.Parent.Background.Percent.Value	
						if script.Head.Value == true then
							local HeadpartHit, HeadposHit, HeaddamageModifier, Headwallbang = SimulateShot(v, "HeadHB", 1.25, 4)
							if HeadpartHit and (HeaddamageModifier * kevlardamage(v) * client.gun.DMG.Value * 4) >= mindamage or HeadpartHit and (HeaddamageModifier * kevlardamage(v) * client.gun.DMG.Value * 4) >= v.Character.Humanoid.Health then
								FireBulletSequence(HeadpartHit, v)
							else
								target = nil
							end
						end
					end
				end
			end
		end)
	end
end)
local BodyThread = coroutine.create(function()
	wait(1/30)
	while true do
		wait(math.random(1, 2)/100)
		pcall(function()
			if rage and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				for i,v in next,plrs:GetPlayers() do
					if v and v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.TeamColor ~= plr.TeamColor then
						local mindamage = script.Parent.Parent.Background.Percent.Value	
						if script.Body.Value == true then
							local PelvispartHit, PelvisposHit, PelvisdamageModifier, Pelviswallbang = SimulateShot(v, "LowerTorso", 1, 1.25)
							local ChestpartHit, ChestposHit, ChestdamageModifier, Chestwallbang = SimulateShot(v, "UpperTorso", 1, 1)
							if PelvispartHit and (PelvisdamageModifier * kevlardamage(v) * client.gun.DMG.Value * 1.25) >= mindamage or PelvisdamageModifier and (PelvisdamageModifier * kevlardamage(v) * client.gun.DMG.Value * 1.25) >= v.Character.Humanoid.Health then
								FireBulletSequence(PelvispartHit, v)
							elseif ChestpartHit and (ChestdamageModifier * kevlardamage(v) * client.gun.DMG.Value * 1) >= mindamage or ChestdamageModifier and (ChestdamageModifier * kevlardamage(v) * client.gun.DMG.Value * 1) >= v.Character.Humanoid.Health then
								FireBulletSequence(ChestpartHit, v)
							else
								target = nil
							end						
						end
					end
				end
			end
		end)	

	end
end)
local ArmsThread = coroutine.create(function()
	wait(1/15)
	while true do
		wait(math.random(1, 2)/50)
		pcall(function()
			if rage and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				for i,v in next,plrs:GetPlayers() do
					if v and v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.TeamColor ~= plr.TeamColor then
						local mindamage = script.Parent.Parent.Background.Percent.Value	
						if script.Arms.Value == true then
							local RightUpperArmpartHit, RightUpperArmposHit, RightUpperArmdamageModifier, RightUpperArmwallbang = SimulateShot(v, "RightUpperArm", 0.5, 1)
							local RightLowerArmpartHit, RightLowerArmposHit, RightLowerArmdamageModifier, RightLowerArmwallbang = SimulateShot(v, "RightLowerArm", 0.5, 1)
							local RightHandpartHit, RightHandposHit, RightHanddamageModifier, RightHandwallbang = SimulateShot(v, "RightHand", 0.1, 1)

							local LeftUpperArmpartHit, LeftUpperArmposHit, LeftUpperArmdamageModifier, LeftUpperArmwallbang = SimulateShot(v, "LeftUpperArm", 0.5, 1)
							local LeftLowerArmpartHit, LeftLowerArmposHit, LeftLowerArmdamageModifier, LeftLowerArmwallbang = SimulateShot(v, "LeftLowerArm", 0.5, 1)
							local LeftHandpartHit, LeftHandposHit, LeftHanddamageModifier, LeftHandwallbang = SimulateShot(v, "LeftHand", 0.1, 1)

							if RightUpperArmpartHit and (RightUpperArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= mindamage or RightUpperArmpartHit and (RightUpperArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(RightUpperArmpartHit, v)
							elseif RightLowerArmpartHit and (RightLowerArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= mindamage or RightLowerArmpartHit and (RightLowerArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(RightLowerArmpartHit, v)
							elseif RightHandpartHit and (RightHanddamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= mindamage or RightHandpartHit and (RightHanddamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(RightHandpartHit, v)
							elseif LeftUpperArmpartHit and (LeftUpperArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= mindamage or LeftUpperArmpartHit and (LeftUpperArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(LeftUpperArmpartHit, v)
							elseif LeftLowerArmpartHit and (LeftLowerArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= mindamage or LeftLowerArmpartHit and (LeftLowerArmdamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(LeftLowerArmpartHit, v)
							elseif LeftHandpartHit and (LeftHanddamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= mindamage or LeftHandpartHit and (LeftHanddamageModifier * kevlardamage(v) * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(LeftHandpartHit, v)
							else
								target = nil
							end
						end
					end
				end
			end
		end)		
	end
end)
local LegsThread = coroutine.create(function()
	wait(1/7.5)
	while true do
		wait(math.random(1, 2)/50)
		pcall(function()
			if rage and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				for i,v in next,plrs:GetPlayers() do
					if v and v ~= plr and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.TeamColor ~= plr.TeamColor then
						local mindamage = script.Parent.Parent.Background.Percent.Value	
						if script.Legs.Value == true then

							local RightUpperLegpartHit, RightUpperLegposHit, RightUpperLegdamageModifier, RightUpperLegwallbang = SimulateShot(v, "RightUpperLeg", 0.5, 0.75)
							local RightLowerLegpartHit, RightLowerLegposHit, RightLowerLegdamageModifier, RightLowerLegwallbang = SimulateShot(v, "RightLowerLeg", 0.5, 0.75)
							local RightFootpartHit, RightFootposHit, RightFootdamageModifier, RightFootwallbang = SimulateShot(v, "RightFoot", 0.1, 1)

							local LeftUpperLegpartHit, LeftUpperLegposHit, LeftUpperLegdamageModifier, LeftUpperLegwallbang = SimulateShot(v, "LeftUpperLeg", 0.5, 0.75)
							local LeftLowerLegpartHit, LeftLowerLegposHit, LeftLowerLegdamageModifier, LeftLowerLegwallbang = SimulateShot(v, "LeftLowerLeg",0.5, 0.75)
							local LeftFootpartHit, LeftFootposHit, LeftFootdamageModifier, LeftFootwallbang = SimulateShot(v, "LeftFoot",0.1, 0.75)

							if RightUpperLegpartHit and (RightUpperLegdamageModifier * client.gun.DMG.Value) >= mindamage or RightUpperLegpartHit and (RightUpperLegdamageModifier * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(RightUpperLegpartHit, v)
							elseif RightLowerLegpartHit and (RightLowerLegdamageModifier * client.gun.DMG.Value) >= mindamage or RightLowerLegpartHit and (RightLowerLegdamageModifier * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(RightLowerLegpartHit, v)
							elseif RightFootpartHit and (RightFootdamageModifier * client.gun.DMG.Value) >= mindamage or RightFootpartHit and (RightFootdamageModifier * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(RightFootpartHit, v)
							elseif LeftUpperLegpartHit and (LeftUpperLegdamageModifier * client.gun.DMG.Value) >= mindamage or LeftUpperLegpartHit and (LeftUpperLegdamageModifier * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(LeftUpperLegpartHit, v)
							elseif LeftLowerLegpartHit and (LeftLowerLegdamageModifier * client.gun.DMG.Value) >= mindamage or LeftLowerLegpartHit and (LeftLowerLegdamageModifier * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(LeftLowerLegpartHit, v)
							elseif LeftFootpartHit and (LeftFootdamageModifier * client.gun.DMG.Value) >= mindamage or LeftFootpartHit and (LeftFootdamageModifier * client.gun.DMG.Value) >= v.Character.Humanoid.Health then
								FireBulletSequence(LeftFootpartHit, v)
							else
								target = nil
							end
						end
					end
				end
			end
		end)	
	end
end)
coroutine.resume(HeadThread)
coroutine.resume(BodyThread)
coroutine.resume(ArmsThread)
coroutine.resume(LegsThread)
local function onButtonActivated()
	if rage == false then
		rage = true
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
	else
		rage = false
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	end
end

button.Activated:Connect(onButtonActivated)
end))
