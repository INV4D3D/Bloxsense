LocalScript1238.Name = "setup"
LocalScript1238.Parent = ScreenGui0
table.insert(cors,sandbox(LocalScript1238,function()
local valid_chars = {}

local function set_valid(x, y)
	for i = string.byte(x), string.byte(y) do
		table.insert(valid_chars, string.char(i))
	end
end

local function random_string(length)
	local s = {}
	for i = 1, length do s[i] = valid_chars[math.random(1, #valid_chars)] end
	return table.concat(s)
end

set_valid('a', 'z')
set_valid('A', 'Z')
set_valid('0', '9')

spawn(function()
	game["Run Service"].RenderStepped:Connect(function()
		script.Parent.Name = "[$$$] "..random_string(4).."_"..random_string(4).."_"..random_string(4).."_".." [$$$]"	
	end)
end)

local Meta = getrawmetatable(game)
setreadonly(Meta,false)
local oldNamecall = Meta.__namecall
Meta.__namecall = newcclosure(function(self,...)
	local Arguments = {...}
	local Method = getnamecallmethod()
	if self == game.Players.LocalPlayer and Method == "Kick" and game.PlaceId == 301549746 then
		print("[bloxsense] session tainted kick attempt detected")
		return
	end
	return oldNamecall(self,unpack(Arguments))
end)
makefolder("bloxsense_configs")

print("[bloxsense] Injected")
local runservice = game:GetService("RunService")
game["Run Service"].RenderStepped:Connect(function()
	game.Players.LocalPlayer.PlayerGui.GUI.SuitZoom.Visible = false
	game.Players.LocalPlayer.PlayerGui.GUI.SuitZoom.BackgroundTransparency = 1
	for i,v in next, game.Players.LocalPlayer.PlayerGui.GUI.SuitZoom:GetDescendants() do
		v.Visible = false
	end
end)
end))
