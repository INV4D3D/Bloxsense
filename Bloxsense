LocalScript1001.Name = "Ambience"
LocalScript1001.Parent = ImageButton999
table.insert(cors,sandbox(LocalScript1001,function()
local button = script.Parent
local toggled = false
local lighting = game.Lighting
local toggled = false

spawn(function()
	wait(3)
	while wait() do
		if toggled == true then
			local Color = script.Parent.Color.BackgroundColor3	
			local Color2 = script.Parent.Color2.BackgroundColor3
			game.Lighting.Ambient = Color
			game.Lighting.OutdoorAmbient = Color2
		else
			game.Lighting.Ambient = Color3.fromRGB(152, 152, 152)
			game.Lighting.OutdoorAmbient = Color3.fromRGB(116, 116, 116)
		end
	end
end)

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	end
end

button.Activated:Connect(onButtonActivated)


end))
