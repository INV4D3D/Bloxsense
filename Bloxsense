LocalScript927.Name = "Function"
LocalScript927.Parent = ImageButton925
table.insert(cors,sandbox(LocalScript927,function()
local button = script.Parent
local toggled = false
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)
		plr.Character.FakeHead:Destroy()
		plr.Character.HeadHB:Destroy()	
		while toggled == true do
			wait()
			pcall(function()
				plr.Character.FakeHead:Destroy()
				plr.Character.HeadHB:Destroy()
			end)				
		end		
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
	end	
end	
button.Activated:Connect(onButtonActivated)
end))
