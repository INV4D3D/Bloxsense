ImageButton28.Name = "RemoveRecoil"
ImageButton28.Parent = Frame21
ImageButton28.Position = UDim2.new(0.0919811353, 0, 0.0577374548, 0)
ImageButton28.Size = UDim2.new(0, 12, 0, 12)
ImageButton28.BackgroundColor = BrickColor.new("Dark taupe")
ImageButton28.BackgroundColor3 = Color3.new(0.290196, 0.290196, 0.290196)
ImageButton28.BorderColor3 = Color3.new(0.137255, 0.137255, 0.137255)
ImageButton28.ZIndex = 2
ImageButton28.Image = "rbxassetid://5761429802"
ImageButton28.ImageTransparency = 0.25
LocalScript29.Name = "NoRecoil"
LocalScript29.Parent = ImageButton28
table.insert(cors,sandbox(LocalScript29,function()
local button = script.Parent
local toggled = false
local loop
local cbClient = getsenv(game.Players.LocalPlayer.PlayerGui.Client)
local recoilXX
local recoilYY

local function onButtonActivated()
	if toggled == false then
		toggled = true
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(181, 255, 42)	
		recoilYY = cbClient.RecoilY
		recoilXX = cbClient.RecoilX
	loop=game:GetService("RunService"):BindToRenderStep("NoRecoil", 100, function()
			cbClient.resetaccuracy()
			cbClient.RecoilX = 0
			cbClient.RecoilY = 0
		end)
	else
		toggled = false
		script.Parent.BackgroundTransparency = 0
		script.Parent.BackgroundColor3 = Color3.fromRGB(74, 74, 74)
		loop:Disconnect()
		cbClient.RecoilX = recoilXX
		cbClient.RecoilY = recoilYY
	end
end
button.Activated:Connect(onButtonActivated)




end))
