		script.Parent.Parent.Ragebot.Ragebot.Arms.Value = false
		script.Parent.Parent.Ragebot.Ragebot.Legs.Value = false
	end
	if script.Parent.Selection.Text == "Head,  Body" then
		script.Parent.Parent.Ragebot.Ragebot.Head.Value = true
		script.Parent.Parent.Ragebot.Ragebot.Body.Value = true
		script.Parent.Parent.Ragebot.Ragebot.Arms.Value = false
		script.Parent.Parent.Ragebot.Ragebot.Legs.Value = false
	end
	if script.Parent.Selection.Text == "Head, Body, Arms" then
