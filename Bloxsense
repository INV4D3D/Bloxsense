table.insert(cors,sandbox(LocalScript1115,function()
local button = script.Parent
local toggled = false

local function onButtonActivated()
	if toggled == false then
		toggled = true
		-- Instances:

		local Frame = Instance.new("Frame")
		local ImageButton = Instance.new("ImageButton")
		local Frame_2 = Instance.new("Frame")
		local Mult = Instance.new("ImageButton")
		local Frame_3 = Instance.new("Frame")
		local BindableEvent = Instance.new("BindableEvent")
		
		--Properties:

		Frame.Parent = script.Parent
		Frame.AnchorPoint = Vector2.new(0.5, 0.5)
		Frame.BackgroundColor3 = Color3.fromRGB(59, 59, 59)
		Frame.BorderColor3 = Color3.fromRGB(35, 35, 35)
		Frame.BorderSizePixel = 3
		Frame.Position = UDim2.new(6.084, 0, 10.0916443, 0)
		Frame.Size = UDim2.new(9.96, 0, 19.7949677, 0)
		Frame.Visible = false
		Frame.ZIndex = 9

		ImageButton.Parent = Frame
		ImageButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ImageButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ImageButton.BorderSizePixel = 0
		ImageButton.Position = UDim2.new(0.161123127, 0, 0.0245737191, 0)
		ImageButton.Size = UDim2.new(0.8197276, 0, 0.949999988, 0)
		ImageButton.ZIndex = 10
		ImageButton.Image = "rbxassetid://2499070690"

		Frame_2.Parent = ImageButton
		Frame_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Frame_2.BackgroundTransparency = 1.000
		Frame_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Frame_2.BorderSizePixel = 4
		Frame_2.Selectable = true

		Mult.Name = "Mult"
		Mult.Parent = Frame
		Mult.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Mult.BorderSizePixel = 0
		Mult.ClipsDescendants = true
		Mult.Position = UDim2.new(0.0149982842, 0, 0.0249474477, 0)
		Mult.Size = UDim2.new(0.125691563, 0, 0.949999988, 0)
		Mult.ZIndex = 10
		Mult.Image = "rbxassetid://2499075935"

		Frame_3.Parent = Mult
		Frame_3.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Frame_3.BackgroundTransparency = 1.000
		Frame_3.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Frame_3.BorderSizePixel = 2
		Frame_3.Position = UDim2.new(-0.5, 0, 0, 0)
		Frame_3.Size = UDim2.new(1.5, 0, 0.00100000005, 0)
		
		BindableEvent.Parent = Frame
		BindableEvent.Name = "ColorChanged"
		-- Scripts:

		local function MOTDEGK_fake_script() -- Frame.Color 
			local script = Instance.new('LocalScript', Frame)
			local Players = game:GetService("Players");local Player = Players.LocalPlayer;local Mouse = Player:GetMouse();local SG = script.Parent;local But = script.Parent.ImageButton;local Scale = 255;local Parts = 6;local R = 255;local G = 0;local B = 0;local Rx = 255;local Gx = 0;local Bx = 0;local Ry = 255;local Gy = 0;local By = 0;local Mult = script.Parent.Mult;local Current = script.Parent.Parent;
			local function Color()
				pcall(function()
				local X = math.ceil((Mouse.X-But.AbsolutePosition.X)/But.AbsoluteSize.X*Scale*Parts)
				local PosX = (Mouse.X-But.AbsolutePosition.X)/But.AbsoluteSize.X
				if X < 0 then
					X = 0
				elseif X > Scale*6 then
					X = Scale*6
				end
				if X <= Scale then
					Rx = Scale
					Gx = 0
					Bx = X
				elseif X <= Scale*2 then
					Rx = Scale*2-X
					Gx = 0
					Bx = Scale
				elseif X <= Scale*3 then
					Rx = 0
					Gx = X-Scale*2
					Bx = Scale
				elseif X <= Scale*4 then
					Rx = 0
					Gx = Scale
					Bx = Scale*4-X
				elseif X <= Scale*5 then
					Rx = X-Scale*4
					Gx = Scale
					Bx = 0
				elseif X <= Scale*6 then
					Rx = Scale
					Gx = Scale*6-X
					Bx = 0
				end
				local Y = math.ceil((Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y*Scale)
				local PosY = (Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y
				if Y < 0 then
					Y = 0
				elseif Y > Scale then
					Y = Scale
				end
				Ry = Rx+math.ceil(Y/Scale*(Scale-Rx))
				Gy = Gx+math.ceil(Y/Scale*(Scale-Gx))
				By = Bx+math.ceil(Y/Scale*(Scale-Bx))
				R = math.ceil(Ry*(1-Mult.Frame.Position.Y.Scale))
				G = math.ceil(Gy*(1-Mult.Frame.Position.Y.Scale))
				B = math.ceil(By*(1-Mult.Frame.Position.Y.Scale))
				if PosX < 0 then
					PosX = 0
				elseif PosX > 1 then
					PosX = 1
				end
				if PosY < 0 then
					PosY = 0
				elseif PosY > 1 then
					PosY = 1
				end
				Current.BackgroundColor3 = Color3.fromRGB(R, G, B)
				script.Parent.ColorChanged:Fire(Color3.fromRGB(R,G,B))
				end)
			end;
			local MouseMove;
			local MouseRoll;
			But.MouseButton1Down:Connect(function()
				Color()
				MouseMove = Mouse.Move:Connect(function()
					Color()
				end)
			end);
			Mult.MouseButton1Down:Connect(function()
				local Y = (Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y
				if Y < 0 then
					Y = 0
				elseif Y > 1 then
					Y = 1
				end
				Mult.Frame.Position = UDim2.new(0,0,Y,0)
				R = math.ceil(Ry*(1-Mult.Frame.Position.Y.Scale))
				G = math.ceil(Gy*(1-Mult.Frame.Position.Y.Scale))
				B = math.ceil(By*(1-Mult.Frame.Position.Y.Scale))
				Current.BackgroundColor3 = Color3.fromRGB(R, G, B)
				MouseRoll = Mouse.Move:Connect(function()
					local Y = (Mouse.Y-But.AbsolutePosition.Y)/But.AbsoluteSize.Y
					if Y < 0 then
						Y = 0
					elseif Y > 1 then
						Y = 1
					end
					Mult.Frame.Position = UDim2.new(0,0,Y,0)
					R = math.ceil(Ry*(1-Mult.Frame.Position.Y.Scale))
					G = math.ceil(Gy*(1-Mult.Frame.Position.Y.Scale))
					B = math.ceil(By*(1-Mult.Frame.Position.Y.Scale))
					Current.BackgroundColor3 = Color3.fromRGB(R, G, B)
					script.Parent.ColorChanged:Fire(Color3.fromRGB(R,G,B))
				end)

			end);
			Mult.MouseButton1Up:Connect(function()
				if MouseRoll ~= nil then
					MouseRoll:Disconnect()
				end
				if MouseMove ~= nil then
					MouseMove:Disconnect()
				end
			end);
			But.MouseButton1Up:Connect(function()
				if MouseRoll ~= nil then
					MouseRoll:Disconnect()
				end
				if MouseMove ~= nil then
					MouseMove:Disconnect()
				end
			end);
			Mouse.Button1Up:Connect(function()
				if MouseRoll ~= nil then
					MouseRoll:Disconnect()
				end
				if MouseMove ~= nil then
					MouseMove:Disconnect()
				end
			end);
		end
		coroutine.wrap(MOTDEGK_fake_script)()
		local colorpicker = script.Parent:WaitForChild("Frame")
		colorpicker.Visible = true
	else
		toggled = false
		local colorpicker = script.Parent:WaitForChild("Frame")
		colorpicker.Visible = false
		colorpicker:Destroy()
	end
end

button.Activated:Connect(onButtonActivated)
end))
